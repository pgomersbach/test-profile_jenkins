- job:
    name: <%= @name %>-start
    description: 'Auto generated job, do not edit this job through the web!'
    project-type: freestyle
    block-downstream: false
    scm:
    - git:
        skip-tag: true
        url: <%= @jobgiturl %>
    triggers:
      - pollscm: 'H/15 * * * *'
    builders:
      - shell:
          cd $WORKSPACE;
          bundle install --path vendor/bundle;
          bundle exec rake syntax;
    properties:
      - delivery-pipeline:
          stage: Stage
          task: Task
    logrotate:
      daysToKeep: 3
      numToKeep: 2
