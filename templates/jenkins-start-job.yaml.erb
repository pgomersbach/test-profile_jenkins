- job:
    name: <%= @name %>-start
    description: 'Auto generated job, do not edit this job through the web!'
    project-type: freestyle
    block-downstream: false
    scm:
    - git:
        skip-tag: true
        url: <%= @jobgiturl %>
    triggers:
      - pollscm: 'H/5 * * * *'
    builders:
      - shell:
          cd $WORKSPACE;
          BUNDLE_PATH="$JENKINS_HOME/shared/bundle";
          bundle install --path $BUNDLE_PATH --without development system_tests;
          bundle exec rake syntax
    properties:
      - delivery-pipeline:
          stage: Stage
          task: Task
    logrotate:
      daysToKeep: 3
      numToKeep: 2
